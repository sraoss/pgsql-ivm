#: help.c:434
#, c-format
msgid "  fieldsep\n"
"    field separator for unaligned output (default \"%s\")\n"
msgstr "  fieldsep\n"
"    розділювач полів для не вирівняного виводу (за замовчуванням \"%s\")\n"

#: help.c:437
#, c-format
msgid "  fieldsep_zero\n"
