msgid "error in WAL record at %X/%X: %s"
msgstr "ошибка в записи WAL в позиции %X/%X: %s"

#: pg_waldump.c:1185
#, c-format
msgid "Try \"%s --help\" for more information.\n"
msgstr "Для дополнительной информации попробуйте \"%s --help\".\n"

